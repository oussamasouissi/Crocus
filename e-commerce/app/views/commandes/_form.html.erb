
<%= render 'layouts/navigation' %>
<!--================Header Menu Area =================-->
<div class = 'row'>
<div class= 'col-xs-12'>
<%= form_for @commande do |f| %>
  <div class="form-group">
    <div class="col-sm-2 control-label" >
        <%=  f.label :etat %>
    </div>    
    <div class: "col-sm-8" >
      <%= f.text_field :etat,  class: "form-control" , placeholder: "etat commande", autofocus: true %>
    </div>
  </div>
  <div class="form-group">
   <div class="col-sm-2 control-label" >
      <%=  f.label :quantité %>
    </div>  
     <div class: "col-sm-8" >
      <%= f.text_field :quantité, rows: 10, class: "form-control", placeholder: "quantité"  %>
    </div>
  </div>  
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10" >
     <%= f.submit class: 'btn btn-primary btn-lg' %>
    </div>
  </div>  
<% end %>
<div class="col-xs-4 col-xs-offset-4">
</div>
</div>
</div> 



