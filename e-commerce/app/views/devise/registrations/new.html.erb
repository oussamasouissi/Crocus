<div class="panel panel-default">
  <div class="panel-heading"><h2>Sign up</h2></div>
  <div class="panel-body">

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render "devise/shared/error_messages", resource: resource %>
      <div class="form-group">
        <%= f.label :email %><br />
        <%= f.email_field :email, rows: 10, class: "form-control", placeholder: "enter email" %>
      </div>


      <div class="form-group">
        <%=  f.label :Name %>
        <%= f.text_field :Name,  class: "form-control" , placeholder: "enter name", autofocus: true %>
      </div>

      <div class="form-group">
        <%=  f.label :UserName %>
        <%= f.text_field :UserName,  class: "form-control" , placeholder: "enter Username", autofocus: true %>
      </div>

      <div class="form-group">
        <%=  f.label :CIN %>
        <%= f.text_field :CIN,  class: "form-control" , placeholder: "enter CIN", autofocus: true %>
      </div>

      <div class="form-group">
        <%=  f.label :téléphone %>
        <%= f.text_field :téléphone,  class: "form-control" , placeholder: "enter phone", autofocus: true %>
      </div>


      <div class="form-group">
        <%=  f.label :adresse %>
        <%= f.text_field :adresse,  class: "form-control" , placeholder: "enter address", autofocus: true %>
      </div>
      </div>

      <div class="form-group">
        <div class="col-sm-2 control-label" >
          <%=  f.label :code_postal %>
        </div>
        <div class="col-sm-8" >
          <%= f.text_field :code_postal,  class: "form-control" , placeholder: "enter code postal", autofocus: true %>
        </div>
      </div>

      <div class="field">
        <%= f.label "sexe" %><br />
        <div class="controls">
          <%= f.select(:sexe, options_for_select([['Monsieur','Monsieur'],['Madame','Madame']])) %>
        </div>
      </div>
      <h4>Si vous etes un client particulier enterez votre matricule fiscale </h4>
      <div class="form-group">
        <div class="col-sm-2 control-label" >

        </div>
        <div class="col-sm-8" >
          <%= f.text_field :matriculeFiscale,  class: "form-control" , placeholder: "enter matriculeFiscale", autofocus: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-2 control-label" >
          <%=  f.label :description %>
        </div>
        <div class="col-sm-8" >
          <%= f.text_field :description,  class: "form-control" , placeholder: "enter description", autofocus: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-2 control-label" >
          <%=  f.label :nomSociete %>
        </div>
        <div class="col-sm-8" >
          <%= f.text_field :nomSociete,  class: "form-control" , placeholder: "enter nomSociete", autofocus: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-2 control-label" >
          <%=  f.label :numFournisseur %>
        </div>
        <div class="col-sm-8" >
          <%= f.text_field :numFournisseur,  class: "form-control" , placeholder: "enter le numéro fournisseur", autofocus: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-8" >
          <%= f.hidden_field :status, value: "enAttente" %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-8" >
          <%= f.hidden_field :roles, value: "client" %>
        </div>
      </div>


      <div class="form-group">
        <%= f.label :password %>
        <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "new-password" , class: "form-control", placeholder: "enter password"%>
      </div>

      <div class="form-group">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control", placeholder: " Password confirmation" %>
      </div>


      <div class="actions">
        <%= f.submit "Sign up" , class: 'btn btn-primary'%>
      </div>
    <% end %>
    <div class="panel-footer"><%= render "devise/shared/links" %></div>
    </div>






    





          

